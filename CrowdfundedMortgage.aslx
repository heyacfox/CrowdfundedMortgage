  <function name="ImageOrderLogic">
    game.temp_front = NoOne
    game.temp_back = NoOne
    if (ListContains(ScopeVisible(), Chrys)) {
      if (ListContains(ScopeVisible(), Steve)) {
        game.temp_front = Chrys
        game.temp_back = Steve
      }
      else if (ListContains(ScopeVisible(), Amand)) {
        game.temp_front = Chrys
        game.temp_back = Amand
      }
      else {
        game.temp_back = Chrys
      }
    }
    if (game.temp_back = NoOne) {
      if (ListContains(ScopeVisible(), Steve)) {
        if (ListContains(ScopeVisible(), Amand)) {
          game.temp_front = Steve
          game.temp_back = Amand
        }
        else {
          game.temp_back = Steve
        }
      }
    }
    if (game.temp_back = NoOne) {
      if (ListContains(ScopeVisible(), Amand)) {
        game.temp_back = Amand
      }
    }
    if (game.temp_front = NoOne) {
      if (ListContains(ScopeVisible(), fundee.currently_talking_to)) {
        game.temp_front = fundee.currently_talking_to
        game.temp_back = NoOne
      }
    }
    if (fundee.parent = Gallery or fundee.parent = Central Hall or fundee.parent = Patio) {
      change_picture_frame (game.temp_front, game.temp_back, fundee.parent)
    }
  </function>
  <function name="first_check">
    PrintCentered (fundee.worker_id.value + ", I am glad you decided you are up for this challenge. This is not something to be taken lightly")
  </function>
  <function name="final_check"><![CDATA[
    if (fundee.funds > fundee.funds_needed - 1) {
      msg ("Congratulations, you raised a total of $" + fundee.funds + "! It looks like you be able to own your own home after 60 years!")
      foreach (funder, game.myTrueFunderList) {
        if (funder.thanked = true) {
          IncreaseObjectCounter (fundee, "funders")
        }
      }
      msg ("You will be paying to the following [" + fundee.funders + "] funders.")
      foreach (funder, game.myTrueFunderList) {
        if (funder.thanked = true) {
          msg ("Funder: " + funder.alias)
        }
      }
      msg ("\"Your children after you will speak of you in their stories and legends. Prepare for your first step out of commonality and into greatness\"")
      finish
    }
    else {
      msg ("Although you did not raise enough money to finance your mortgage, do not feel ashamed. Only 5% of total participants raise enough funds. For the rest of your life, you are still expected to remain productive while we take 95% of your input for rent. Enjoy the rest of your life.")
      finish
    }
    DisableTimer (pager_buzz2)
  ]]></function>
