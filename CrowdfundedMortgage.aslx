  <function name="tell_finance_topic" parameters="funder">
    set (fundee, "currently_talking_to", funder)
    if (GetBoolean(funder, "tell_finance")) {
      msg ("funder.alias + \"says, \\"You already talked to me about that.\\"\"")
      trust_decrease_small (funder)
    }
    else {
      ShowMenu ("What do you want to tell " + funder.alias + " about your finances", fundee.finance_topics, false) {
        if (result = "how much money your parents have") {
          switch (fundee.currently_talking_to) {
            case (Steve) {
              msg ("\"That's a real shame, you can work on my boat if you need some extra dough to save up for a sailboat\"")
              trust_increase_small (Steve)
              if (ListContains(ScopeVisible(), Chrys)) {
                trust_increase_large (Chrys)
              }
              if (ListContains(ScopeVisible(), Amand)) {
                trust_decrease_small (Amand)
              }
            }
            case (Chrys) {
              msg ("\"Screw them, go make your own future!\"")
              trust_increase_large (Chrys)
              if (ListContains(ScopeVisible(), Steve)) {
                trust_increase_small (Steve)
              }
              if (ListContains(ScopeVisible(), Amand)) {
                trust_decrease_small (Amand)
              }
            }
            case (Amand) {
              msg ("\"Though you may not come from the best of stock, thank you for your confidence.\"")
              trust_increase_small (Amand)
              if (ListContains(ScopeVisible(), Steve)) {
                trust_increase_small (Steve)
              }
              if (ListContains(ScopeVisible(), Chrys)) {
                trust_increase_medium (Chrys)
              }
            }
          }
        }
        else if (result = "how frugal you are with your spending") {
          switch (fundee.currently_talking_to) {
            case (Steve) {
              msg ("\"Brilliant! I know a lot about being frugal. When you are on a ship for 180 days sailing around the world you have to pay close attention to every resource you have.\"")
              trust_increase_medium (Steve)
              if (ListContains(ScopeVisible(), Chrys)) {
                trust_increase_small (Chrys)
              }
              if (ListContains(ScopeVisible(), Amand)) {
                trust_decrease_large (Amand)
              }
            }
            case (Chrys) {
              msg ("\"That's good, I better make my money back and you better not spend it on things that are worthless.\"")
              trust_increase_small (Chrys)
              if (ListContains(ScopeVisible(), Steve)) {
                trust_increase_small (Steve)
              }
              if (ListContains(ScopeVisible(), Amand)) {
                trust_decrease_medium (Amand)
              }
            }
            case (Amand) {
              msg ("\"How uncouth, how can you LIVE like that? With that kind of attitude you simply don't BELONG in high society!\"")
              trust_decrease_large (Amand)
              if (ListContains(ScopeVisible(), Steve)) {
                trust_increase_small (Steve)
              }
              if (ListContains(ScopeVisible(), Chrys)) {
                trust_increase_small (Chrys)
              }
            }
          }
        }
        else if (result = "how you make your credit payments early every month") {
          switch (fundee.currently_talking_to) {
            case (Steve) {
              msg ("\"But then how will you spend it on things that really matter?\"")
              trust_decrease_medium (Steve)
              if (ListContains(ScopeVisible(), Chrys)) {
                trust_increase_medium (Chrys)
              }
              if (ListContains(ScopeVisible(), Amand)) {
                trust_decrease_small (Amand)
              }
            }
            case (Chrys) {
              msg ("\"You're good. You may not make as much money as my crypto-farms, but maybe Dad was onto something here.\"")
              trust_increase_medium (Chrys)
              if (ListContains(ScopeVisible(), Steve)) {
                trust_decrease_small (Steve)
              }
              if (ListContains(ScopeVisible(), Amand)) {
                trust_decrease_medium (Amand)
              }
            }
            case (Amand) {
              msg ("\"Why do you WORRY so much?\"")
              trust_decrease_small (Amand)
              if (ListContains(ScopeVisible(), Steve)) {
                trust_decrease_small (Steve)
              }
              if (ListContains(ScopeVisible(), Chrys)) {
                trust_increase_medium (Chrys)
              }
            }
          }
        }
      }
    }
    SetObjectFlagOn (funder, "tell_finance")
