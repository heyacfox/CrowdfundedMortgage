  <function name="ImageOrderLogic">
    game.temp_front = NoOne
    game.temp_back = NoOne
    if (ListContains(ScopeVisible(), Chrys)) {
      if (ListContains(ScopeVisible(), Steve)) {
        game.temp_front = Chrys
        game.temp_back = Steve
      }
      else if (ListContains(ScopeVisible(), Amand)) {
        game.temp_front = Chrys
        game.temp_back = Amand
      }
      else {
        game.temp_back = Chrys
      }
    }
    if (game.temp_back = NoOne) {
      if (ListContains(ScopeVisible(), Steve)) {
        if (ListContains(ScopeVisible(), Amand)) {
          game.temp_front = Steve
          game.temp_back = Amand
        }
        else {
          game.temp_back = Steve
        }
      }
    }
    if (game.temp_back = NoOne) {
      if (ListContains(ScopeVisible(), Amand)) {
        game.temp_back = Amand
      }
    }
    if (game.temp_front = NoOne) {
      if (ListContains(ScopeVisible(), fundee.currently_talking_to)) {
        game.temp_front = fundee.currently_talking_to
        game.temp_back = NoOne
      }
    }
    if (fundee.parent = Gallery or fundee.parent = Central Hall or fundee.parent = Patio) {
      change_picture_frame (game.temp_front, game.temp_back, fundee.parent)
    }
  </function>
