    SetTimeout (game.time_cycle_count_start * 60 *  3 / 12) {
      change_room (Chrys, Central Hall)
    }
    SetTimeout (game.time_cycle_count_start * 60 *  4 / 12) {
      change_room (Chrys, Gallery)
    }
    SetTimeout (game.time_cycle_count_start * 60 *  5 / 12) {
      change_room (Steve, Central Hall)
    }
    SetTimeout (game.time_cycle_count_start * 60 *  7 / 12) {
      change_room (Amand, Gallery)
    }
    SetTimeout (game.time_cycle_count_start * 60 *  8 / 12) {
      change_room (Steve, Patio)
    }
